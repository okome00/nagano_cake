<%# ジャンル検索画面 %>
<table class="table border border-dark text-center">
  <thead>
    <tr>
      <th class="border-bottom border border-dark">ジャンル検索</th>
    </tr>
  </thead>
  <tbody class="table-borderless">
    <% genres.each do |genre| %>
    <tr>
      <td><%= genre.name %></td>
    </tr>
    <% end %>
  </tbody>
</table>
